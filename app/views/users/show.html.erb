<% provide(:title, 'Show') %>
<p id="notice"><%= notice %></p>
<div id="userShow">
<h1><%= @user.name %>'s Profile</h1>
<p>
  <strong>Name:</strong>
  <h2><%= @user.name %></h2>
</p>

<p>
  <strong>Email:</strong>
  <h2><%= @user.email %></h2>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> <a href="#">|</a>
<%= link_to 'Back', users_path %>
</div>
